<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <title>Lab Monkeys</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>
    <script src="https://kit.fontawesome.com/e20aa6b03c.js" crossorigin="anonymous"></script>


    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <!--Font awesome-->
    <link rel="stylesheet" href="~/fonts/font-awesome/css/font-awesome.min.css" />

    <!--Image slider slick script-->
    <script src="~/Content/slick/slick.min.js"></script>

    <!--Image slider styling-->
    <link href="~/Content/image-slider.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/slick/slick.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/slick/slick-theme.css" rel="stylesheet" type="text/css" />

    <!--PDP styling-->
    <link rel="stylesheet" href="~/Content/PDP/pdp.style.css" />
    <link rel="stylesheet" href="~/Content/PDP/carousel.style.css" />

    <!--Hero block styling-->
    <link href="~/Content/hero-block.css" rel="stylesheet" type="text/css" />

    <script type="text/javascript">
        var baseUrl = "@(String.Format("{0}://{1}/", Request.Url.Scheme, Request.Url.Authority))"
    </script>

    @RenderSection("Styles", required: false)

</head>

<body >
    <div id="vue-app">
        <div id="header">
            @Html.Action("Index", "Navbar")
        </div>

        <div class="container body-content" style="min-height:75vh">
            @RenderBody()
            
        </div>
        <footer style="margin-top: 2rem"> @Html.Action("GetFooter", "FooterBlock")</footer>
       

        <script src="~/Content/dist/js/app.js"></script>

        <script>
            new Vue({}).$mount('#vue-app')
        </script>
    </div>
</body>
</html>